      <script>
// URL server untuk menerima data log
const LOG_SERVER = "https://your-server.com/logger"; 

// Kirim data ke server
function sendLog(data) {
  fetch(LOG_SERVER, {
    method: "POST",
    headers: {"Content-Type": "application/json"},
    body: JSON.stringify(data)
  }).catch(err => console.error("Gagal kirim log:", err));
}

// Log: error JavaScript
window.addEventListener("error", function(event) {
  sendLog({
    type: "js-error",
    message: event.message,
    source: event.filename,
    line: event.lineno,
    col: event.colno,
    time: Date.now()
  });
});

// Log: waktu loading halaman
window.addEventListener("load", function() {
  const timing = performance.getEntriesByType("navigation")[0];
  sendLog({
    type: "page-load",
    url: window.location.href,
    loadTime: timing.loadEventEnd,
    domContentLoaded: timing.domContentLoadedEventEnd,
    time: Date.now()
  });
});

// Log: klik user
document.addEventListener("click", function(e) {
  sendLog({
    type: "click",
    tag: e.target.tagName,
    text: e.target.innerText.slice(0,50), // hanya 50 karakter
    time: Date.now()
  });
});
</script>

        
    </script>
